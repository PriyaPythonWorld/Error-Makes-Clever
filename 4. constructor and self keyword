class laptop:
    price = 0
    ram = ""
    def __init__(self):
        self.price = 0
        self.ram = ""
        self.processor = ""
        print("demo constructor")
    def display(self):
        print("Ram:", self.ram)
        print("Processor:", self.processor)

hp = laptop()
dell = laptop()

hp.price = 50000
print(hp.price)

hp.ram = "8gb"
hp.processor = "i5"

dell.ram = "16gb"
dell.processor = "i7"

print(hp.ram)

hp.display()
dell.display()
